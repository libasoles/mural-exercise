{
  "name": "mural",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "npm run build:sw && next build",
    "start": "next start",
    "build:sw": "babel libraries/serviceWorker/serviceWorker.js --out-file static/service-worker.js",
    "lint": "eslint --fix '{pages,components,libraries}/**/*.js'",
    "lint:css": "stylelint './components/**/*.js'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run lint:css"
    }
  },
  "dependencies": {
    "next": "latest",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "styled-components": "^4.1.3",
    "styled-system": "^4.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "babel-eslint": "10.0.1",
    "babel-plugin-styled-components": "^1.10.0",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "prettier": "^1.16.4",
    "prop-types": "^15.7.2",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.2"
  },
  "license": "ISC",
  "author": "alexis sgarbossa"
}
